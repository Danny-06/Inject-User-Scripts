{
  "typeAcquisition": {
    "include": ["chrome"],
    "exclude": ["core-js"]
  },
  "compilerOptions": {
    "lib": ["ESNext", "DOM"],
    "module": "ESNext",
    "target": "ESNext",
    "strictNullChecks": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitAny": false,
    "useUnknownInCatchVariables": true,
    // "checkJs": true,

    "jsx": "react-jsx",
		"jsxImportSource": "preact",
  },
}
